{
  "name": "dccreative-nestjs-keycloak-connect",
  "version": "0.1.0",
  "description": "keycloak-nodejs-connect module for Nest (Fork)",
  "author": "David Christiansen <coding@davedoes.net>",
  "license": "MIT",
  "scripts": {
    "start:dev": "tsc -w",
    "clean": "rimraf dist",
    "build": "tsc",
    "release": "release-it",
    "release:alpha": "release-it --preReleaseId=alpha --npm.tag=next --github.preRelease",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint ./src --ext .ts"
  },
  "keywords": [
    "nestjs",
    "keycloak",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DavidChristiansen/dccreative-nest-keycloak-connect.git"
  },
  "bugs": "https://github.com/DavidChristiansen/dccreative-nest-keycloak-connect/issues",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "dist/index.d.ts": [
        "dist/index.d.ts"
      ],
      "*": [
        "dist/*"
      ]
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "peerDependencies": {
    "@nestjs/common": ">=6.0.0 <12.0.0",
    "@nestjs/core": ">=6.0.0 <12.0.0",
    "@nestjs/graphql": ">=6"
  },
  "devDependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/graphql": "^13.1.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.5.2",
    "@types/parse-path": "^7.1.0",
    "@typescript-eslint/eslint-plugin": "8.44.1",
    "@typescript-eslint/parser": "8.44.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "cpr": "3.0.1",
    "graphql": "^16.6.0",
    "prettier": "3.6.2",
    "reflect-metadata": "0.2.2",
    "release-it": "^19.0.5",
    "rimraf": "6.0.1",
    "rxjs": "7.8.2",
    "ts-node": "10.9.2",
    "typescript": "5.9.2"
  },
  "peerDependenciesMeta": {
    "@nestjs/graphql": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "jwk-to-pem": "^2.0.7"
  }
}
